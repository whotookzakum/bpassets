      "TextTable": [
        {
          "Id": {
            "IdString": "0",
            "FallbackId": "None",
            "IdNumber": 0
          },
          "Text": "えっと、東が右で……。\r\n左は盾を持つほうで……",
          "MetaData": [
            {
              "speaker_id": "Npc_Pissarie"
            },
            {
              "listener_id": "Player"
            }
          ],
          "Version": {
