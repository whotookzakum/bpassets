            {
              "speaker_id": "Npc_Rabori"
            },
            {
              "listener_id": "Player"
            }
          ],
          "Version": {
            "Versions": []
          }
        },
        {
          "Id": {
            "IdString": "35",
            "FallbackId": "None",
            "IdNumber": 35
          },
          "Text": "<span size=\"Big\">……！　…………ッ？</>",
          "MetaData": [
            {
              "speaker_id": "Npc_Nappo"
            },
            {
              "listener_id": "Npc_Rabori"
            }
          ],
          "Version": {
            "Versions": []
          }
        },
        {
          "Id": {
            "IdString": "36",
            "FallbackId": "None",
            "IdNumber": 36
          },
          "Text": "よーし、よし。\r\nじゃあ、そのまま向こうへ行くんだ",
