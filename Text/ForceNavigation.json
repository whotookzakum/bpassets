          "Version": {
            "Versions": [
              1,
              6,
              0
            ]
          }
        },
        {
          "Id": {
            "IdString": "advboard_12",
            "FallbackId": "None",
            "IdNumber": 0
          },
          "Text": "アドベンチャーボードは\n旅に役立つさまざまな報酬が獲得できるので、\n積極的に挑戦してみましょう",
          "MetaData": [],
          "Version": {
            "Versions": [
              1,
              6,
              0
            ]
          }
        }
      ]
